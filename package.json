{
  "name": "@eik/cli",
  "version": "3.1.30",
  "description": "CLI tool for publishing assets to an Eik server",
  "main": "./classes/index.js",
  "types": "./types/classes/index.d.ts",
  "type": "module",
  "bin": {
    "eik": "index.js"
  },
  "files": [
    "CHANGELOG.md",
    "package.json",
    "readme.md",
    "index.js",
    "commands",
    "classes",
    "utils",
    "formatters",
    "types"
  ],
  "scripts": {
    "clean": "rimraf .tap node_modules types",
    "test": "cross-env HTTP_PORT=0 LOG_LEVEL=fatal tap --timeout 90 --jobs=1 --disable-coverage",
    "test:integration": "cross-env HTTP_PORT=0 LOG_LEVEL=fatal tap --timeout 90 --jobs=1 --disable-coverage test/integration/**/*.test.mjs",
    "test:unit": "cross-env HTTP_PORT=0 LOG_LEVEL=fatal tap --timeout 90 --jobs=1 --disable-coverage test/*.test.mjs",
    "test:tasks": "cross-env HTTP_PORT=0 LOG_LEVEL=fatal tap --timeout 90 --jobs=1 --disable-coverage test/tasks/*.test.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "fix": "run-s lint:fix format:fix",
    "types": "run-s types:module types:test",
    "types:module": "tsc",
    "types:test": "tsc --project tsconfig.test.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eik-lib/cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@eik/common": "5.1.11",
    "abslog": "2.4.4",
    "bytes": "3.1.2",
    "date-fns": "4.1.0",
    "form-data": "4.0.5",
    "gzip-size": "7.0.0",
    "ora": "9.0.0",
    "rimraf": "6.1.2",
    "semver": "7.7.3",
    "tar": "7.5.2",
    "tinyrainbow": "3.0.3",
    "yargs": "18.0.0",
    "yargs-parser": "22.0.0"
  },
  "overrides": {
    "semantic-release-slack-bot": {
      "micromatch": "4.0.8"
    }
  },
  "devDependencies": {
    "@eik/eslint-config": "1.0.23",
    "@eik/prettier-config": "1.0.1",
    "@eik/semantic-release-config": "1.0.10",
    "@eik/service": "5.1.6",
    "@eik/sink-memory": "2.0.13",
    "@eik/typescript-config": "1.0.0",
    "@types/yargs": "17.0.35",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "fastify": "5.6.2",
    "fs-extra": "11.3.2",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "semantic-release-slack-bot": "4.0.2",
    "tap": "21.5.0",
    "typescript": "5.9.3"
  }
}
